{
    "name": "code-snippets",
    "description": "Manage code snippets running on a WordPress-powered site through a graphical interface.",
    "homepage": "https://wordpress.org/plugins/code-snippets",
    "version": "3.6.6",
    "main": "src/dist/edit.js",
    "directories": {
        "test": "tests"
    },
    "scripts": {
        "test": "eslint && npm run phpcs",
        "build": "webpack",
        "watch": "webpack --watch",
        "bundle": "ts-node scripts/bundle.ts",
        "phpcs": "src/vendor/bin/phpcs -s --colors ./phpcs.xml",
        "phpcbf": "src/vendor/bin/phpcbf ./phpcs.xml",
        "version": "ts-node scripts/version.ts",
        "version-dev": "npm version --git-tag-version=false --preid=dev",
        "version-alpha": "npm version --git-tag-version=false --preid=alpha",
        "version-beta": "npm version --git-tag-version=false --preid=beta",
        "version-rc": "npm version --git-tag-version=false --preid=rc"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/codesnippetspro/code-snippets.git"
    },
    "keywords": [
        "wordpress-plugin"
    ],
    "author": "Shea Bunge <shea@codesnippets.pro> (https://sheabunge.com)",
    "license": "GPL-2.0-or-later",
    "bugs": {
        "url": "https://github.com/codesnippetspro/code-snippets/issues"
    },
    "dependencies": {
        "@codemirror/fold": "^0.19.4",
        "@wordpress/api-fetch": "^7.13.0",
        "@wordpress/block-editor": "^14.8.0",
        "@wordpress/blocks": "^14.2.0",
        "@wordpress/components": "^28.13.0",
        "@wordpress/data": "^10.13.0",
        "@wordpress/dom-ready": "^4.13.0",
        "@wordpress/i18n": "^5.13.0",
        "@wordpress/icons": "^10.13.0",
        "@wordpress/server-side-render": "^5.13.0",
        "axios": "^1.7.8",
        "classnames": "^2.5.1",
        "codemirror": "^5.29",
        "codemirror-colorpicker": "^1.9.80",
        "php-parser": "^3.2.1",
        "prismjs": "^1.29.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-select": "^5.8.3"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.2.0",
        "@eslint/js": "^9.16.0",
        "@stylistic/eslint-plugin": "^2.11.0",
        "@tsconfig/node18": "^18.2.4",
        "@types/archiver": "^6.0.3",
        "@types/codemirror": "^5.60.15",
        "@types/jquery": "^3.5.32",
        "@types/node": "^22.10.1",
        "@types/prismjs": "^1.26.5",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@types/rtlcss": "^3.5.4",
        "@types/tinymce": "^4.6.9",
        "@types/web": "^0.0.185",
        "@types/wordpress__editor": "^14.12.0",
        "@typescript-eslint/eslint-plugin": "^8.17.0",
        "@typescript-eslint/parser": "^8.17.0",
        "@wordpress/babel-preset-default": "^8.13.0",
        "archiver": "^7.0.1",
        "autoprefixer": "^10.4.20",
        "babel-loader": "^9.2.1",
        "babel-plugin-prismjs": "^2.1.0",
        "css-loader": "^7.1.2",
        "cssnano": "^7.0.6",
        "eslint": "^9.16.0",
        "eslint-import-resolver-typescript": "^3.6.3",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.0.0",
        "eslint-webpack-plugin": "^4.2.0",
        "glob": "^11.0.0",
        "globals": "^15.13.0",
        "mini-css-extract-plugin": "^2.9.2",
        "postcss": "^8.4.49",
        "postcss-hexrgba": "^2.1.0",
        "postcss-load-config": "^6.0.1",
        "postcss-loader": "^8.1.1",
        "rtlcss": "^4.3.0",
        "sass": "^1.81.1",
        "sass-loader": "^16.0.3",
        "style-loader": "^4.0.0",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.17.0",
        "webpack": "^5.96.1",
        "webpack-cli": "^5.1.4",
        "webpack-merge": "^6.0.1",
        "webpack-remove-empty-scripts": "^1.0.4"
    },
    "overrides": {
        "eslint": "^9.16.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    }
}
